<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>



	<extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.gemoc.gemoc_modeling_workbench.samples.category"
            name="Gemoc modeling workbench examples"
            parentCategory="org.eclipse.ui.Examples"/>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.gemoc.gemoc_modeling_workbench.samples.category"
            class="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramModelExampleWizard"
            finalPerspective="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling"
            hasPages="false"
            icon="icons/IconeGemocModel-16.png"
            id="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramModelExampleWizard"
            name="ActivityDiagram Model examples (concurrent / sequential)"
            preferredPerspectives="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling">
         <description>
            T⁮his project contains some ActivityDiagram models that can be executed if you have installed the one or both ActivityDiagram languages (Sequential and/or Concurrent).
         </description>
      </wizard>
      <category
            id="org.gemoc.gemoc_language_workbench.samples.category"
            name="Gemoc language workbench examples"
            parentCategory="org.eclipse.ui.Examples"/>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.gemoc.gemoc_language_workbench.samples.category"
            class="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramConcurrentLanguageExampleWizard"
            finalPerspective="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling"
            hasPages="false"
            icon="icons/IconeGemocLanguage-16.png"
            id="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramConcurrentLanguageExampleWizard"
            name="ActivityDiagram Concurrent Language"
            preferredPerspectives="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling,fr.obeo.dsl.viewpoint.ui.tools.perspective.design">
         <description>
            T⁮his set of projects feature an ActivityDiagram concurrent language which is made executable using Gemoc.
         </description>
      </wizard><wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.gemoc.gemoc_language_workbench.samples.category"
            class="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramSequentialLanguageExampleWizard"
            finalPerspective="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling"
            hasPages="false"
            icon="icons/IconeGemocLanguage-16.png"
            id="org.gemoc.activitydiagram.sampledeployer.wizards.ActivityDiagramSequentialLanguageExampleWizard"
            name="ActivityDiagram Sequential Language"
            preferredPerspectives="fr.obeo.dsl.viewpoint.ui.tools.perspective.modeling,fr.obeo.dsl.viewpoint.ui.tools.perspective.design">
         <description>
            T⁮his set of projects feature an ActivityDiagram sequential language which is made executable using Gemoc.
         </description>
      </wizard>
   </extension>
 <extension
       point="org.eclipse.help.toc">
    <toc
          file="toc.xml"
          primary="false">
    </toc>
 </extension>
 
</plugin>
